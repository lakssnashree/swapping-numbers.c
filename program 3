#include<stdio.h>
int main()
{       int ch;
        printf("enter any number btw 1-5 to get your food");
        scanf("%d",&ch);
        switch(ch)
       { case 1:
           printf("food item - burger \n price -Rs239");
           break;
        case 2: 
           printf("food item - pizza\n price -Rs129");
           break;
         case 3:
           printf("food item - sandwich \n price - Rs149");
           break;
          case 4:
           printf("food item - french fries \n price -Rs99");
           break;
           case 5:
           printf("food item - pasta \n price -Rs179");
           break;
           default:
              printf(" food item is out of stock");
          }
        return 0;
}
