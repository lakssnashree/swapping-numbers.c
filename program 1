#include<stdio.h>
int main()
{
  int a,b;  //a=10,b=20	
	//Input values
	printf("Enter the value of a");
	scanf("%d",&a);//10
	printf("Enter the value of b");
	scanf("%d",&b);	//20
	
	// before swapping
	printf("Before swapping a: %d, b: %d\n",a,b);//Before swapping a: 10, b: 20
	
	//swapping numbers
	a = a+b;	//a=30
	b = a-b;	//b=10
	a = a-b; 	//a=20
	
	// after swapping 
	printf("After swapping a: %d, b: %d\n",a,b);//After swapping a: 20, b: 10
  return 0;
  }

